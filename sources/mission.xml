<?xml version="1.0"?>

<mission lastly="loiter" mode="restart">
	<command name="waypoint" id="1" latitude="234.4324234" longitude="12.123123123"/>
	<control name="delay" seconds="100"/>
	<command name="waypoint" id="2" altitude="50" latitude="198.984234" longitude="98.217462178"/>
	<control name="delay" seconds="150"/>
	<command name="waypoint" id="3" latitude="567.5675432" longitude="175.23568558"/>
	<control name="while" variable="altitude" condition="less" value="75">
		<control name="set" variable="speed" mode="relative" value="15"/>
		<control name="set" variable="altitude" mode="relative" value="15"/>
		<control name="if" variable="wp_eta" condition="grater" value="65">
			<command name="rth" id="14"/>
			<control id="5" name="delay" seconds="20"/>
		</control>
	</control>
	<command name="loiter" id="24" altitude="86" rounds="12"/>
	<command name="land" id="24" altitude="86"/>
</mission>